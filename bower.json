{
  "name": "ai-frontend",
  "private": true,
  "dependencies": {
    "bootstrap-sass": "~3.3.5",
    "modernizr": "~2.8.1",
    "bourbon": "~4.2.3",
    "magnific-popup": "~1.0.0",
    "picturefill": "~2.3.1",
    "lazysizes": "~1.1.5",
    "jquery": "~1",
    "requirejs": "~2.1.20",
    "requirejs-domready": "~2.0.1",
    "galleria": "~1.4.2",
    "neat": "~1.7.2"
  },
  "overrides": {
    "bootstrap-sass": {
